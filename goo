-- Pop-up animation function
local function animateFrame(frame, targetVisible)
    local tweenService = game:GetService("TweenService")
    local startSize = frame.Size
    local startPosition = frame.Position

    local targetSize = UDim2.new(0, 260, 0, 400)
    local targetPosition = UDim2.new(0.5, -130, 0.5, -200)

    if targetVisible then
        frame.Size = UDim2.new(0, 0, 0, 0)
        frame.Position = UDim2.new(0.5, 0, 0.5, 0)
    else
        targetSize = UDim2.new(0, 0, 0, 0)
        targetPosition = UDim2.new(0.5, 0, 0.5, 0)
    end

    local tweenInfo = TweenInfo.new(
        0.5, -- Time
        Enum.EasingStyle.Back, -- Easing style
        Enum.EasingDirection.Out, -- Easing direction
        0, -- Repeat count
        false, -- Reverses
        0 -- Delay time
    )

    local sizeTween = tweenService:Create(frame, tweenInfo, {Size = targetSize})
    local positionTween = tweenService:Create(frame, tweenInfo, {Position = targetPosition})

    frame.Visible = true
    sizeTween:Play()
    positionTween:Play()

    sizeTween.Completed:Connect(function()
        if not targetVisible then
            frame.Visible = false
            frame.Size = startSize
            frame.Position = startPosition
        end
    end)
end

local animations = {}

local function loadAnimations(character)
    local humanoid = character:WaitForChild("Humanoid")
    for name, id in pairs(animationsData) do
        local animation = Instance.new("Animation")
        animation.AnimationId = id
        animations[name] = humanoid:LoadAnimation(animation)
        animations[name].Looped = true
    end
end

loadAnimations(player.Character or player.CharacterAdded:Wait())
player.CharacterAdded:Connect(loadAnimations)

local function toggleAnimation(animationName)
    local animationTrack = animations[animationName]
    if animationTrack.IsPlaying then
        animationTrack:Stop()
    else
        for _, anim in pairs(animations) do
            anim:Stop()
        end
        animationTrack:Play()
    end
end

local buttons = {}

local function updateButtons(searchFilter)
    for _, button in pairs(buttons) do
        button:Destroy()
    end
    buttons = {}

    local yOffset = 0
    for name, _ in pairs(animationsData) do
        if not searchFilter or name:lower():find(searchFilter:lower()) then
            local button = Instance.new("TextButton")
            button.Text = name .. " Animation"
            button.Font = Enum.Font.GothamBold
            button.TextSize = 14
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Size = UDim2.new(1, 0, 0, 40)
            button.Position = UDim2.new(0, 0, 0, yOffset)
            button.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
            button.Parent = scrollFrame

            local btnCorner = Instance.new("UICorner")
            btnCorner.CornerRadius = UDim.new(0, 8)
            btnCorner.Parent = button

            button.MouseButton1Click:Connect(function()
                toggleAnimation(name)
            end)

            yOffset = yOffset + 50
            table.insert(buttons, button)
        end
    end
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, yOffset)
end

searchButton.MouseButton1Click:Connect(function()
    scrollFrame.Visible = true
    updateButtons(searchBox.Text)
end)

toggleButton.MouseButton1Click:Connect(function()
    local targetVisibility = not mainFrame.Visible
    animateFrame(mainFrame, targetVisibility)
end)

-- Apply Animation Speed
setSpeedButton.MouseButton1Click:Connect(function()
    local speed = tonumber(speedBox.Text)
    if speed then
        for _, animationTrack in pairs(animations) do
            animationTrack:AdjustSpeed(speed)
        end
    else
        print("Invalid speed")
        -- Optionally, display an error message to the user
    end
end)

updateButtons()
